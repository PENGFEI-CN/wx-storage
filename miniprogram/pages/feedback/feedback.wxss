/* miniprogram/pages/feedback/feedback.wxss */
@import "../../ui.wxss";
page{
  width:100%;
  height: 100%;
}
.page{
  width: 95%;
  height: 95%;
  margin:0 auto;
  display: flex;
  flex-direction: column;
  align-items:center;

}
.weui_cells{
  border: 2rpx solid gray;
  margin-top:300rpx;
}
.weui-footer{
  position: fixed;
  bottom: 10rpx;
}


.container{
    width:100%;
    height:100%;
    overflow: hidden
}
.ripple {
    background-color: rgba(0, 0, 0, 0.6);
    border-radius: 100%;
    height:10px;
    width:10px;
    margin-top: -90px;
    position: absolute;
    -webkit-transform: scale(0);
    overflow: hidden
}
@-webkit-keyframes ripple {
    100% {
    -webkit-transform: scale(12);
    transform: scale(12);
    background-color: transparent;
    }
}
.but{
  margin-top:200rpx;
  width:300rpx;
  background-color: rgb(226, 238, 215);
}

